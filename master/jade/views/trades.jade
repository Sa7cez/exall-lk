        ul.nav.nav-tabs.nav-justified(role='tablist')
          li.active(role='presentation')
            a.bb0(href='#buy', aria-controls='buy', role='tab', data-toggle='tab', data-localize="trades.BUY")
          li(role='presentation')
            a.bb0(href='#sell', aria-controls='sell', role='tab', data-toggle='tab', data-localize="trades.SELL")
        // Tab panes
        .tab-content
          #buy.tab-pane.active(role='tabpanel') 
            .container-fluid
              .row
                .col-lg-12
                    .table-responsive
                      table#ordersBuy.table.table-striped
                        thead
                          tr
                            th(data-localize="trades.ORDER").order_name
                            th(data-localize="trades.DATE").date
                            th.btc BTC
                            th(data-localize="trades.AMOUNT").amount
                            th(data-localize="trades.COUNTRY").country
                            th(data-localize="trades.STATUS").status
                            th.full-info  
                        tbody

